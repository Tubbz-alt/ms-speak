# (From Packages)
cp squid/squid.conf /usr/local/squid/etc/
cp c_icap/configure ../c_icap-0.5.5
sudo mkdir ../c_icap-0.5.5/services/msp
cp c_icap/services/msp ../c_icap-0.5.5/services/msp
cp c_icap/services/Makefile.am ../c_icap-0.5.5/services/Makefile.am
cp c_icap/c-icap.conf /usr/local/etc/

sudo mkdir /usr/local/share/c_icap/templates/msp
sudo mkdir /usr/local/share/c_icap/templates/msp/en			
sudo cp c_icap/services/msp/MSP_RESPONSE /usr/local/share/c_icap/templates/msp/en